# PostgreSQL Configuration
DATABASE_HOST=postgres
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=your_secure_password
DATABASE_NAME=astrbot_plugins

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# GitHub OAuth App (Register at https://github.com/settings/applications/new)
# Authorization callback URL: http://your-domain/auth/github/callback
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GITHUB_WEBHOOK_SECRET=your-super-secret-webhook-key

# Application URLs
APP_URL=http://localhost:3000
API_URL=http://localhost:3001

# LLM Provider Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_API_BASE_URL=https://api.openai.com/v1
OPENAI_API_MODEL=gpt-4-turbo

# Environment
NODE_ENV=development
